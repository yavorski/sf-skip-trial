{
  "manifest_version": 3,
  "name": "Skip SF Trial",
  "version": "1.0.0",
  "description": "Skip Sitefinity CMS Trial Screen",
  "icons": {
    "16": "icons/icon-16.png",
    "24": "icons/icon-24.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "64": "icons/icon-64.png",
    "128": "icons/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [ "<all_urls>" ],
      "js": [ "main.js" ],
      "run_at": "document_start"
    }
  ]
}
